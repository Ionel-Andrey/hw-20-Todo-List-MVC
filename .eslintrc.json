{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-alert": 0,
        "linebreak-style": 0,
        "indent": 0,
        "no-use-before-define": 0,
        "no-multi-assign": 0,
        "max-len": 0,
        "max-classes-per-file": 0
    }
}